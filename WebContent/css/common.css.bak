.messeage_box p{
	font:26px/1 'Cuprum','Lucida Sans Unicode', 'Lucida Grande', sans-serif;
	background:url('./../img/header_bg.jpg') repeat-x left bottom #f5f5f5;
	padding: 18px 25px;
	text-shadow: 1px 1px 0 rgba(255, 255, 255, 0.6);
	color:#666;
}

.messeage_box h1{
	letter-spacing:0.3px;
	color:#888;
	text-align: center;
}

.messeage_box p{
	background:#EDEDED;
	font-size:16px;
	line-height:1.4;
	padding-top: 35px;
	text-align:center;
}
.messeage_box img{
	margin-left:165px;
}

.ime_on {
	ime-mode: active;
}

.ime_off {
	ime-mode: disabled;
}

.nw {
	white-space: nowrap;
}

.checkBoxTransForm {
	transform: scale(1.25);
}